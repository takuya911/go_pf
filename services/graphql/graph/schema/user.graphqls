extend type Query {
  user: [User!]!
}

extend type Mutation {
  createUser(input: CreateUser!): User!
}

type User {
  id: ID!
  name: String!
  email: String!
  Password: String!
  createdAt: String!
  updatedAt: String!
  deletedAt: String
}

input CreateUser {
  id: ID!
  name: String!
  email: String!
  Password: String!
}
